{
  "name": "launch-with-ai-frontend",
  "private": true,
  "version": "1.0.0",
  "description": "Launch With AI - Frontend application",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc -p tsconfig.build.json && vite build",
    "lint": "biome check --apply-unsafe src/",
    "lint:ci": "biome check src/",
    "preview": "vite preview",
    "generate-client": "openapi-ts",
    "generate-types": "openapi-ts && npx biome format --write ./src/client",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:run": "vitest run",
    "test:coverage": "vitest run --coverage"
  },
  "dependencies": {
    "@chakra-ui/accordion": "^2.3.1",
    "@chakra-ui/layout": "^2.3.1",
    "@chakra-ui/progress": "^2.2.0",
    "@chakra-ui/react": "^3.16.1",
    "@chakra-ui/stat": "^2.1.1",
    "@chakra-ui/stepper": "^2.3.1",
    "@chakra-ui/tabs": "^3.0.0",
    "@emotion/react": "^11.14.0",
    "@hookform/resolvers": "^5.2.2",
    "@tanstack/react-query": "^5.28.14",
    "@tanstack/react-query-devtools": "^5.28.14",
    "@tanstack/react-router": "1.19.1",
    "axios": "1.7.4",
    "form-data": "4.0.0",
    "i18next": "^25.8.4",
    "i18next-browser-languagedetector": "^8.2.0",
    "next-themes": "^0.4.4",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-error-boundary": "^4.0.13",
    "react-hook-form": "^7.55.0",
    "react-i18next": "^16.5.4",
    "react-icons": "^5.4.0",
    "zod": "^4.1.11"
  },
  "devDependencies": {
    "@biomejs/biome": "1.6.1",
    "@hey-api/openapi-ts": "^0.57.0",
    "@playwright/test": "^1.45.2",
    "@tanstack/router-devtools": "1.19.1",
    "@tanstack/router-vite-plugin": "1.19.0",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/node": "^20.10.5",
    "@types/react": "^18.2.37",
    "@types/react-dom": "^18.2.15",
    "@vitejs/plugin-react-swc": "^3.5.0",
    "@vitest/ui": "^4.0.7",
    "dotenv": "^16.4.5",
    "happy-dom": "^20.0.10",
    "jsdom": "^27.1.0",
    "typescript": "^5.2.2",
    "vite": "^5.4.14",
    "vitest": "^4.0.7"
  }
}
